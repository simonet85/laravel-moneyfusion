{
    "name": "votre-vendor/laravel-moneyfusion",
    "description": "Laravel package for MoneyFusion payment integration",
    "keywords": [
        "laravel",
        "moneyfusion",
        "payment",
        "mobile-money",
        "orange-money",
        "mtn-money",
        "wave",
        "africa",
        "payment-gateway"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Votre Nom",
            "email": "votre@email.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "VotreVendor\\LaravelMoneyFusion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VotreVendor\\LaravelMoneyFusion\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VotreVendor\\LaravelMoneyFusion\\MoneyFusionServiceProvider"
            ],
            "aliases": {
                "MoneyFusion": "VotreVendor\\LaravelMoneyFusion\\Facades\\MoneyFusion"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
