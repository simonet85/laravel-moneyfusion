{
    "name": "simonet85/laravel-moneyfusion",
    "description": "Laravel package for MoneyFusion payment integration",
    "keywords": [
        "laravel",
        "moneyfusion",
        "payment",
        "payment-gateway"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simonet",
            "email": "christian.85@live.fr"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Simonet85\\LaravelMoneyFusion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simonet85\\LaravelMoneyFusion\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simonet85\\LaravelMoneyFusion\\MoneyFusionServiceProvider"
            ],
            "aliases": {
                "MoneyFusion": "Simonet85\\LaravelMoneyFusion\\Facades\\MoneyFusion"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
